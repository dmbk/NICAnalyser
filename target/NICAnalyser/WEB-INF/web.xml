<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>NICAnalyser</display-name>
    <servlet>
        <servlet-name>NICCalculator</servlet-name>
        <servlet-class>com.cse.nicanalyser.servlet.NICCalculator</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NICCalculator</servlet-name>
        <url-pattern>/NICCalculator</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>NICInterpreter.jsp</welcome-file>
        </welcome-file-list>
    </web-app>
